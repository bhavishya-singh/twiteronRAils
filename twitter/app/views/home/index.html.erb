<%= form_tag '/create_tweet' do %>
	<%=text_area_tag :content %>
	<br>
	<%= submit_tag "Create Tweet" %>
<% end %>

<%= form_tag '/', method: :get do %>

<%= text_field_tag 'search' %>
<br>
<%= submit_tag 'search' %>
 <% end %>
<% @tweets.each do |tweet| %>
	<%= render 'tweet', :tweet => tweet%>
<%end%>

<%= link_to "logout", logout_path%>